{
    "manifestVersion": 1,
    "id": "liquidefile-build-release-task",
    "name": "LiquidFile Build and Release Uploader tool",
    "version": "1.0.2",
    "publisher": "AviHadad",
    "public": true,
    "targets": [{
        "id": "Microsoft.VisualStudio.Services"
    }],
    "description": "Tools for building releasing with LiquidFile. Includes one build release task.",
    "categories": [
        "Azure Pipelines"
    ],
    "tags": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "images/icon.png"
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/jessehouwing/azure-pipelines-gate-condition-editor.git"
    },
    "files": [{
        "path": "buildAndReleaseTask"
    }],
    "content": {
        "details": {
            "path": "README.md"
        },
        "license": {
            "path": "LICENSE.md"
        },
        "privacy": {
            "path": "PRIVACY.md"
        }
    },
    "contributions": [{
        "id": "custom-build-release-task",
        "type": "ms.vss-distributed-task.task",
        "targets": [
            "ms.vss-distributed-task.tasks"
        ],
        "properties": {
            "name": "buildAndReleaseTask"
        }
    }]
}