{
    "manifestVersion": 1,
    "id": "liquidefile-build-release-task",
    "name": "LiquidFile Build and Release Uploader tool",
    "version": "1.1.27",
    "publisher": "AviHadad",
    "public": true,
    "targets": [{
        "id": "Microsoft.VisualStudio.Services"
    }],
    "description": "Tools for building releasing with LiquidFile. Includes one build release task.",
    "categories": [
        "Azure Pipelines"
    ],
    "tags": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "images/icon.png"
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/avih75/LiquidFileUploader.git"
    },
    "links": {
        "getstarted": {
            "uri": "https://github.com/avih75/LiquidFileUploader/wiki"
        },
        "support": {
            "uri": "https://github.com/avih75/LiquidFileUploader/issues"
        },
        "license": {
            "uri": "https://github.com/avih75/LiquidFileUploader/LICENSE.md"
        },
        "privacypolicy": {
            "uri": "https://github.com/avih75/LiquidFileUploader/blob/main/PRIVACY.md"
        }
    },
    "files": [{
        "path": "buildAndReleaseTask"
    }],
    "content": {
        "details": {
            "path": "Details.md"
        },
        "license": {
            "path": "LICENSE.md"
        },
        "privacy": {
            "path": "PRIVACY.md"
        }
    },
    "contributions": [{
        "id": "custom-build-release-task",
        "type": "ms.vss-distributed-task.task",
        "targets": [
            "ms.vss-distributed-task.tasks"
        ],
        "properties": {
            "name": "buildAndReleaseTask"
        }
    }]
}